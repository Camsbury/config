     [:kc-h :kc-n :kc-e :kc-i :kc-o]
     [:kc-k :kc-m :kc-comma :kc-dot :kc-slash]]
    :outer
    [[:to :main-lay] :kc-delete [:lt :symbol-lay :kc-rbracket] :kc-f-15]
    :inner
    [[:to :sc2-lay] :kc-app :kc-no]
    :bottom
    [:kc-lgui :kc-lalt :kc-meh :kc-hypr :kc-f-16]
    :thumb
    {:top   [:kc-f-14 :kc-no]
     :main  [[:lt :number-lay :kc-enter] :kc-bspace]
     :inner [:kc-no :kc-lctl]}}}}

